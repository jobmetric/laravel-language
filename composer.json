{
    "name": "jobmetric/laravel-language",
    "description": "It is a standard package for managing different system languages in Laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "language",
        "multi-language"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-language",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.31",
        "spatie/laravel-query-builder": "^6.3",
        "jobmetric/multi-calendar": "^1.2",
        "jobmetric/laravel-event-system": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Language\\": "src/",
            "JobMetric\\Language\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JobMetric\\Language\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Language\\LanguageServiceProvider"
            ],
            "aliases": {
                "Language": "JobMetric\\Language\\Facades\\Language"
            }
        }
    },
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-language/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-language"
    }
}
